if [ -d "$HOME/.cargo/env" ]; then 
  . "$HOME/.cargo/env"
fi

if [ -d "$HOME/.pyenv" ] && [[ $OSTYPE != 'darwin'* ]]; then
  export PYENV_ROOT="$HOME/.pyenv"
  export PATH="$PYENV_ROOT/bin:$PATH"
  eval "$(pyenv init --path)"
fi

if [ -f "$HOME/.pyenv/shims/pdm" ]; then
  eval "$($HOME/.pyenv/shims/pdm --pep582)"
fi
